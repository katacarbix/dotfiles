{
  "autopreview": {
    "image": true,
    "todolist": true,
    "iframe": true,
    "anchor": true,
    "math": true
  },
  "autopreview-domains": [
    "dailymotion.com",
    "deezer.com",
    "dribbble.com",
    "facebook.com",
    "github.com",
    "google.com",
    "instagram.com",
    "pinterest.com",
    "soundcloud.com",
    "spotify.com",
    "twitter.com",
    "vimeo.com",
    "youtube.com"
  ],
  "autopreview-security": true,
  "editor": {
    "font-size": "20px"
  },
  "highlight-modes": "",
  "copy-images-on-html-export": true,
  "preview-template": "default",
  "startup-commands": {
    "showBlocks": false,
    "showHiddenCharacters": false,
    "showTocPane": false,
    "showLastLineIndicator": true,
    "autoCloseBrackets": true,
    "autosave": true
  },
  "spellchecker": {
    "active": true,
    "language": "en_US",
    "src": "/home/reese/.config/Abricotine/app/dict/en_US"
  },
  "tasks": [
    {
      "name": "Export with Pandoc",
      "exec": "pandoc -s %inputFilepath% -o %outputFilepath%"
    },
    {
      "name": "Markdown-magic transforms",
      "exec": "md-magic -p %inputFilepath%"
    },
    {
      "name": "Markdown-TOC transform",
      "exec": "markdown-toc -i %inputFilepath% --maxdepth=3"
    }
  ],
  "theme": "wal",
  "window": {
    "showMenuBar": false
  }
}